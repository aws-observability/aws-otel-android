{
  "aws": {
    "region": "us-east-1",
    "rumAppMonitorId": "YOUR-RUM-APP-MONITOR-ID",
    "rumAlias": "YOUR-RUM-ALIAS"
  },
  "exportOverride": {
    "logs": "http://10.0.2.2:4318/v1/logs",
    "traces": "http://10.0.2.2:4318/v1/traces"
  },
  "telemetry": {
    "activity": { "enabled": true },
    "anr": { "enabled": true },
    "crash": { "enabled": true },
    "fragment": { "enabled": true },
    "network": { "enabled": true },
    "slowRendering": { "enabled": true },
    "startup": { "enabled": true },
    "http": {
      "enabled": true,
      "capturedRequestHeaders": ["request-header"],
      "capturedResponseHeaders": ["response-header"]
    },
    "uiLoad": { "enabled": true },
    "sessionEvents": { "enabled": true }
  },
  "sessionTimeout": 100,
  "sessionSampleRate": 1.0,
  "applicationAttributes": {
    "application.version": "1.0.0",
    "demo": true
  }
}